FROM centos:6
MAINTAINER Example42 GmbH <info@example42.com>

RUN yum install -y epel-release
RUN yum install -y supervisor  && yum clean all

VOLUME /var/log/supervisor


ENTRYPOINT [ "supervisord"   , ""--nodaemon" ]
CMD [ "-c","/etc/supervisord.conf" ]
