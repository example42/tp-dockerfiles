FROM alpine:3.3
MAINTAINER Example42 GmbH <info@example42.com>
RUN echo "[supervisord]\nnodaemon=true\n\n[program:nginx]\ncommand=nginx   -g 'daemon off;'" > /etc/supervisor/conf.d/nginx.conf
EXPOSE 80
